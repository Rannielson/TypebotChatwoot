{
  "name": "typebot-chatwoot-connector",
  "version": "1.0.0",
  "description": "Conector SaaS multi-tenant entre Chatwoot, Typebot e Meta WhatsApp API",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "migrate": "ts-node src/database/migrate.ts",
    "migrate:check": "ts-node src/database/check-migration.ts",
    "seed": "ts-node src/database/seed.ts",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "setup": "bash start.sh",
    "dev:all": "bash start-dev.sh",
    "docker:up": "cd docker && docker-compose up -d",
    "docker:down": "cd docker && docker-compose down",
    "docker:logs": "cd docker && docker-compose logs -f"
  },
  "keywords": [
    "chatwoot",
    "typebot",
    "whatsapp",
    "chatbot",
    "saas",
    "multi-tenant"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/node-cron": "^3.0.11",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.66.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^4.2.1",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "redlock": "^5.0.0-beta.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/form-data": "^2.5.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
